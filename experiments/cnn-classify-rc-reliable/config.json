{
    "dirname": "cnn-classify-rc-reliable",
    "debug": false,
    "seed": 42,
    "n_splits": 10,
    "gpu_id": 2,
    "epochs": 200,
    "batch_size": 512,
    "conv1d": {
        "filters": [
            128,
            128,
            128
        ],
        "kernel_sizes": [
            3,
            3,
            3
        ],
        "dilation_rates": [
            1,
            1,
            1
        ]
    },
    "lr": 1e-3,
    "es_patience": 10,
    "schedular": {
        "monitor": "val_loss",
        "factor": 0.75,
        "patience": 5,
        "min_lr": 1e-5,
        "min_delta": 0,
        "verbose": 1
    },
    "cut": 35
}